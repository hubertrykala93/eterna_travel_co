{
  "name": "eterna_travel_co",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "apps/eterna_travel_co/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/eterna_travel_co",
        "index": "apps/eterna_travel_co/src/index.html",
        "main": "apps/eterna_travel_co/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/eterna_travel_co/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/eterna_travel_co/public",
            "output": "assets"
          }
        ],
        "styles": ["apps/eterna_travel_co/src/styles.scss"]
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "4kb",
              "maximumError": "8kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "options": {
        "port": 8100
      },
      "continuous": true,
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "eterna_travel_co:build:production"
        },
        "development": {
          "buildTarget": "eterna_travel_co:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "eterna_travel_co:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": [
          "apps/eterna_travel_co/**/*.ts",
          "apps/eterna_travel_co/**/*.spec.ts",
          "apps/eterna_travel_co/**/*.scss",
          "apps/eterna_travel_co/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "reportsDirectory": "../../coverage/apps/eterna_travel_co"
      }
    },
    "serve-static": {
      "continuous": true,
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "eterna_travel_co:build",
        "spa": true
      }
    }
  }
}
